#!/bin/sh
#
# Triggers the sound setup script which configures the volume levels.

SETUP_SCRIPT="/usr/bin/sound_setup.sh"

case "$1" in
    start)
        echo -n "Running sound setup script"
        $SETUP_SCRIPT && echo "OK" || (echo "FAIL"; exit 1)
        ;;
    *)
        echo "Usage: $0 {start}"
        ;;
esac

exit 0
